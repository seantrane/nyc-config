{
  "check-coverage": true,
  "per-file": false,
  "branches": 90,
  "functions": 90,
  "lines": 90,
  "statements": 90,
  "watermarks": {
    "branches": [
      85,
      95
    ],
    "functions": [
      85,
      95
    ],
    "lines": [
      85,
      95
    ],
    "statements": [
      85,
      95
    ]
  },
  "extension": [
    ".ts",
    ".tsx"
  ],
  "exclude": [
    "__tests__",
    "**/*.d.ts",
    "**/*.spec.ts",
    "**/*.test.ts",
    "coverage"
  ],
  "include": [
    "**/*.ts"
  ],
  "reporter": [
    "html",
    "lcov",
    "text"
  ],
  "all": true
}
